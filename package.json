{
  "name": "@pinia/plugin-debounce",
  "version": "0.0.0",
  "description": "Config based debouncing for actions in your Pinia stores",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "sideEffects": false,
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "scripts": {
    "dev": "mocha -w",
    "test": "pnpm run test:cov",
    "test:types": "tsc --build ./tsconfig.json",
    "test:cov": "nyc mocha",
    "build": "pnpm build:esm & pnpm build:iife & wait",
    "build:esm": "tsup src/index.ts --format esm,cjs --dts --target es2020",
    "build:iife": "tsup src/index.ts --format iife --minify --global-name PiniaDebounce --target es2020"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "conventional-changelog-cli": "^2.1.1",
    "esbuild-register": "^2.6.0",
    "expect": "^27.0.6",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "pinia": "^2.0.0-rc.3",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "ts-debounce": "^3.0.0",
    "ts-node": "^10.1.0",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5",
    "vue": "^3.2.0-beta.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/pinia-plugin-debounce.git"
  },
  "bugs": {
    "url": "https://github.com/posva/pinia-plugin-debounce/issues"
  },
  "homepage": "https://github.com/posva/pinia-plugin-debounce#readme"
}
